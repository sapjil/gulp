{%- extends "./../_templates/_layout.njk" -%}
{%- import "./../_templates/_macro/_macro.njk" as macro -%}

{% block content %}
<div class="wrap">
  <h1>index 페이지 입니다!</h1>
  {{ macro.image( "image_box", "/common/images/google.jpeg", "메인 이미지 입니다.", width="200px" ) }}
	<div>
		<ul class="list_wrap">
    {%- for listData in list_data %}
		<li class="xxx eee-{{listData.id}}">
			<a href="{{listData.href}}">{{listData.text}}-{{listData.id}}</a>
		</li>
    {%- endfor %}
		</ul>
		{# contents area #}
		<div>
			{%- for listData in item_data %}
			<p class="paragraph_wrap">
			<span class="xxx-{{listData.id}}">{{listData.id}}</span>{{listData.text}}
			</p>
			{%- endfor %}
		</div>
		<div>
			<div class="swiper content2">
				<!-- Additional required wrapper -->
				<div class="swiper-wrapper">
					<!-- Slides -->
					<div class="swiper-slide">Slide 1</div>
					<div class="swiper-slide">Slide 2</div>
					<div class="swiper-slide">Slide 3</div>
					<div class="swiper-slide">Slide 4</div>
					<div class="swiper-slide">Slide 5</div>
				</div>
				<!-- If we need pagination -->
				<div class="swiper-pagination"></div>

				<!-- If we need navigation buttons -->
				<div class="swiper-button-prev"></div>
				<div class="swiper-button-next"></div>

				<!-- If we need scrollbar -->
				<div class="swiper-scrollbar"></div>
			</div>
		</div>
  </div>
</div>
{%- endblock %}

{% block footerscript %}
	<script>
		const swiper2 = new swiperCustom({
			customClassName: '.content2',
			bannerAutoplay: false,
			spaceBetween: 20,
			slidesPerView: 3,
			slidesPerView1: 2,
			slidesPerView2: 1,
			loop: false,
		});
	</script>
{% endblock %}
{% include "./../_templates/_include/_htmlend.njk" %}
